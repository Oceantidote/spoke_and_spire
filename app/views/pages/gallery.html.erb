<div class="container pv-big">
  <h1 class="tc">Have a look at what we've been up to lately</h1>
  <div class="row pv-big">
    <% @photos.each do |photo| %>
      <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 p-0">
        <%= link_to photo_path(photo) do  %>
          <div class="article-container">
              <div class="article-img-holder image" title="<%= photo.title %>" style="background-image: url(<%= url_for photo.photo %>)"></div>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
